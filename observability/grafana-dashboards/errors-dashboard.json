{
  "title": "Timmy â€“ Errors",
  "uid": "timmy-errors",
  "schemaVersion": 38,
  "version": 1,
  "timezone": "browser",
  "refresh": "1m",
  "templating": {
    "list": [
      {
        "name": "slug",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "Loki"
        },
        "refresh": 2,
        "query": "label_values(slug)",
        "hide": 0,
        "includeAll": false,
        "multi": false
      },
      {
        "name": "phase",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "Loki"
        },
        "refresh": 2,
        "query": "label_values({slug=\"$slug\"}, phase)",
        "hide": 0,
        "includeAll": false,
        "multi": false
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Phase errors (5m)",
      "type": "timeseries",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({slug=\"$slug\", level=\"ERROR\", phase=\"$phase\"}[5m]))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Recent errors (phase)",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{slug=\"$slug\", level=\"ERROR\", phase=\"$phase\"}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ]
}
