{
  "title": "Timmy - Logs",
  "uid": "timmy-logs",
  "schemaVersion": 38,
  "version": 1,
  "timezone": "browser",
  "refresh": "1m",
  "templating": {
    "list": [
      {
        "name": "slug",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "Loki"
        },
        "refresh": 2,
        "query": "label_values(slug)",
        "hide": 0,
        "includeAll": true,
        "multi": false,
        "allValue": ".+"
      },
      {
        "name": "run_id",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "Loki"
        },
        "refresh": 2,
        "query": "label_values({slug=\"$slug\"}, run_id)",
        "hide": 0,
        "includeAll": true,
        "multi": false,
        "allValue": ".+"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Log run (slug + run_id)",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{slug=~\"${slug:regex}\", run_id=~\"${run_id:regex}\"}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      }
    }
  ]
}
